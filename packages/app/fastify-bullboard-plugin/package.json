{
	"name": "@lokalise/fastify-bullboard-plugin",
	"version": "1.0.0",
	"type": "module",
	"engines": {
		"node": ">=20.13.0"
	},
	"exports": {
		".": "./README.md"
	},
	"scripts": {
		"init": "",
		"setup": "",
		"build": "tsc",
		"test": "cross-env NODE_ENV=test vitest",
		"test:coverage": "npm test -- --coverage",
		"pretest:ci": "npm run docker:start:ci && cross-env NODE_ENV=test npm run build",
		"test:ci": "npm run test:coverage",
		"test:ci:teardown": "docker compose down",
		"lint": "biome check . && tsc --noEmit",
		"lint:fix": "biome check --write",
		"docker:start:dev": "docker compose up -d redis bull-board",
		"docker:start:ci": "docker compose up -d --quiet-pull redis",
		"docker:stop:dev": "docker compose down",
		"prepublishOnly": "npm run build",
	},
	"dependencies": {
		"@bull-board/api": "^6.7.9",
		"@bull-board/fastify": "^6.7.9",
		"@fastify/auth": "^5.0.2",
		"@fastify/basic-auth": "^6.0.3",
		"@lokalise/backend-http-client": "^3.0.0",
		"@lokalise/background-jobs-common": "^10.4.2",
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@lokalise/biome-config": "^1.5.0",
		"@types/node": "^22.13.0",
		"@vitest/coverage-v8": "^2.1.8",
		"@fastify/awilix": "^7.0.0",
		"@fastify/schedule": "^6.0.0",
		"@lokalise/fastify-extras": "^26.1.1",
		"@lokalise/node-core": "^13.1.0",
		"awilix": "^12.0.4",
		"awilix-manager": "^6.1.0",
		"bullmq": "5.33.1",
		"cross-env": "^7.0.3",
		"fastify": "^5.2.1",
		"fastify-graceful-shutdown": "^4.0.1",
		"fastify-metrics": "^12.1.0",
		"fastify-plugin": "^5.0.1",
		"ioredis": "^5.4.1",
		"pino": "^9.6.0",
		"toad-scheduler": "^3.0.1",
		"ts-deepmerge": "^7.0.2",
		"pino-pretty": "^13.0.0",
		"tsx": "^4.19.3",
		"type-fest": "^4.33.0",
		"typescript": "5.7.3",
		"vitest": "^2.0.5",
		"zod": "^3.23.8"
	}
}
