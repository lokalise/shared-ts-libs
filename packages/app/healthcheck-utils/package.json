{
    "name": "@lokalise/healthcheck-utils",
    "version": "4.0.1",
    "license": "Apache-2.0",
    "files": ["dist/**", "LICENSE.md", "README.md"],
    "type": "module",
    "main": "./dist/index.js",
    "homepage": "https://github.com/lokalise/shared-ts-libs",
    "repository": {
        "type": "git",
        "url": "https://github.com/lokalise/shared-ts-libs.git"
    },
    "exports": {
        ".": "./dist/index.js",
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "rimraf dist && tsc --project tsconfig.build.json",
        "clean": "rimraf dist",
        "lint": "biome check . && tsc",
        "lint:fix": "biome check --write",
        "docker:start:ci": "docker compose up -d --quiet-pull redis",
        "test": "vitest run",
        "pretest:ci": "npm run docker:start:ci",
        "test:ci": "vitest run --coverage",
        "test:ci:teardown": "docker compose down",
        "prepublishOnly": "npm run build",
        "package-version": "echo $npm_package_version",
        "postversion": "biome check --write package.json"
    },
    "dependencies": {
        "@lokalise/node-core": "^14.0.0",
        "@supercharge/promise-pool": "^3.2.0",
        "redis-semaphore": "^5.6.2",
        "toad-cache": "^3.7.0"
    },
    "peerDependencies": {
        "@lokalise/background-jobs-common": ">=12.0.0",
        "ioredis": "^5.4.1",
        "prom-client": "^15.1.3",
        "toad-scheduler": "^3.1.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@lokalise/background-jobs-common": "^12.3.0",
        "@lokalise/biome-config": "^2.0.0",
        "@lokalise/node-core": "^13.6.0",
        "@lokalise/tsconfig": "^1.3.0",
        "@vitest/coverage-v8": "^3.1.2",
        "ioredis": "^5.6.1",
        "prom-client": "^15.1.3",
        "rimraf": "^6.0.1",
        "toad-scheduler": "^3.1.0",
        "typescript": "5.8.3",
        "vitest": "^3.1.2"
    }
}
