        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@lokalise/api-contracts": "^4.1.0",
        "@lokalise/biome-config": "^1.5.0",
        "@lokalise/tsconfig": "~1.0.2",
        "@vitest/coverage-v8": "^3.0.9",
        "fastify-type-provider-zod": "^4.0.2",
        "rimraf": "^6.0.1",
        "typescript": "5.8.2",
        "vitest": "^3.0.9"
